    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^3.1.0",
    "eslint-config-node": "^4.0.0",
    "eslint-config-prettier": "^6.0.0",
    "micro-dev": "^3.0.0",
    "nyc": "^15.0.0",
    "sinon": "^8.1.1",
    "xo": "^0.25.0",
    "semantic-release": "^17.0.2"
  },
  "peerDependencies": {
    "micro": "^9.3.0"
